<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Bootstrap core CSS -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet">
    <!-- Material Design Bootstrap -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.5.13/css/mdb.min.css" rel="stylesheet">
    <!-- Custom Styles -->
    <link rel="stylesheet" href="/stylesheets/custom.css" />
    <link rel="stylesheet" href="/stylesheets/style.css" />
</head>
<body class="bg-gradient-light d-block">
<div class = "fixed-top">
    <div class="sticky-top navbar navbar-dark bg-dark text-white d-flex flex-nowrap align-items-start">
        <div class = "p-2 d-none d-sm-block disable-select"><a href="/" style="text-decoration: none; color:white;"><h2><small class = "lead">C S 3 5 5 </small></a></h2></div>
        <div class = "p-2 col-8 mx-auto align-self-center"><input class = "form-control" type="text" placeholder="Enter your search query here . . ." required></div>
        <div class = "p-2 align-self-end">
            <form method="post" action="/api/search/">
                <button id = "SUBMIT-ID" type="button" class="btn btn-primary">Search</button>
            </form>
        </div>

    </div>
</div>
<div class="container mt-5">
    <div class="card card-body border-primary mh-100 ">
        <div id="output">
            <div class = "d-flex flex-column mb-3">
                <% data.map(function(element){ %>
                <div>
                    <a href = "<%= element.url%>"><%= element.title%></a>
                    <p class="ml-5 text-muted">
                        <%= element.desc %>
                    </p>
                </div>
                <%}); %>
            </div>
        </div>
    </div>
</div>
</body>
<div id = "included-javascript" class="d-none">
    <!-- JQuery -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <!-- Bootstrap tooltips -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.4/umd/popper.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.3/js/bootstrap.min.js"></script>
    <!-- MDB core JavaScript -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.5.13/js/mdb.min.js"></script>
</div>
</html>

